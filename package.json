{
  "name": "repeat-grid-tabber",
  "version": "0.1.0",
  "description": "Quickly tab through and edit text in a repeat grid in Adobe XD",
  "main": "main.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "webpack --mode development",
    "dev": "nodemon -w src -e ts,tsx,json,css -w webpack.config.js -x yarn build",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arniebradfo/xd-plugin-repeat-grid-tabber.git"
  },
  "keywords": [
    "adobe",
    "xd",
    "plugin",
    "repeat",
    "grid",
    "tab"
  ],
  "author": "James Bradford",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arniebradfo/xd-plugin-repeat-grid-tabber/issues"
  },
  "homepage": "https://github.com/arniebradfo/xd-plugin-repeat-grid-tabber#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@adobexd/typings": "AdobeXD/typings#master",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "nodemon": "^1.19.1",
    "style-loader": "^0.23.1",
    "typescript": "^4.5.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  }
}
