{
  "name": "repeat-grid-tabber",
  "version": "0.1.0",
  "description": "Quickly tab through and edit text in a repeat grid in Adobe XD",
  "main": "main.js",
  "scripts": {
    "start": "yarn run watch",
    "watch": "nodemon -w src -e ts,tsx,css -w webpack.config.js -x yarn build",
    "build": "webpack --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arniebradfo/xd-plugin-repeat-grid-tabber.git"
  },
