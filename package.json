    "adobe",
    "xd",
    "plugin",
    "repeat",
    "grid",
    "tab"
  ],
  "author": "James Bradford",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arniebradfo/xd-plugin-repeat-grid-tabber/issues"
  },
  "homepage": "https://github.com/arniebradfo/xd-plugin-repeat-grid-tabber#readme",
  "dependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/core": "^7.16.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "css-loader": "^6.5.1",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "9.2.6",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "yarn": "^1.22.17"
  }
